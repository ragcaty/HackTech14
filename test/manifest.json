{
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/bomb19.png",
      "128": "images/bomb128.png"
    },
    "default_title": "Tick-Tock"
  },
  "description": "Tracks website usage and blocks specific ones",
  "icons": {
    "19": "images/bomb19.png",
    "128": "images/bomb128.png"
  },
  "manifest_version": 2,
  "name": "Tick-Tock",
  "options_page": "options.html",
  "permissions": [
   "activeTab",
   "alarms",
   "notifications",
   "tabs",
   "webRequest",
   "webRequestBlocking",
   "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1"
}
